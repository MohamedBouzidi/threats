%h1.page-header Dashboard
.row
  .col-md-12
    .panel.panel-default
      .panel-heading Main
      .panel-body
        %canvas#gauge
        %canvas#gauge-low
        %canvas#gauge-medium
.row
  .col-md-12
    .panel.panel-default
      .panel-heading Real-Time Attacks
      .panel-body
        .table-responsive
          %table.table
            %thead
              - @columns.each do |column|
                %th= column
            %tbody.attacks-container
              - @rows.each do |row|
                %tr
                  - row.length.times do |index|
                    %td= row[index]
